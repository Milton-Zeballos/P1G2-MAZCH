function EjercicioUno(){
    var nro = 14
    var i = 1
    while(i < nro){
        if(i % 2 == 0){
        }else{
            alert(i)
        }
        i = i + 1
    }
    alert("FIN")
}

function EjercicioDos(){
    var A = 10
    var B = 7
    if(A>B){
        alert("El mayor de " +A+ " y " +B+ " es " +A)
    }else{
        alert("El mayor de " +A+ " y " +B+ " es " +B)
    }
}

function EjercicioSeis(){
    var A = 5
    var B = 4
    var C = 9
    if(A>B){
        if(A>C){
            alert("El mayor de " +A+","+B+","+C+ " es " +A)
        }else{
            alert("El mayor de " +A+","+B+","+C+ " es " +C)
        }
    }else{
        if(b>c){
            alert("El mayor de " +A+","+B+","+C+ " es " +B)
        }else{
            alert("El mayor de " +A+","+B+","+C+ " es " +C)
        }
    }
}
function AreaVolumenCilindro(){
    var R = 5
    var H = 12
    var A = 0
    var V = 0
    var pi = 3.1416
    A = (2*pi*R*H)+(2*pi*(R*R))
    V = pi*(R*R)*H
    alert("El area es " + A)
    alert("El volumen es " + V)
}
//Codigo de programacion - Fin
